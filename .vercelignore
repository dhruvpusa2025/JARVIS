# Vercel Deployment Ignore
# This file tells Vercel which files to exclude from deployment

# Development files
.env
.env.*
!.env.example

# Testing
tests/
phpunit.xml
.phpunit.result.cache

# Development dependencies (node_modules will be rebuilt)
node_modules/
.npm/

# IDE
.idea/
.vscode/
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db

# Git
.git/
.gitignore
.gitattributes

# Documentation
README.md

# Development tools
.editorconfig
.php-cs-fixer.php
.php-cs-fixer.cache

# Package manager lock files (except composer.lock - needed for PHP dependencies)
package-lock.json
yarn.lock
pnpm-lock.yaml
